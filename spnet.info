; Style (compiled scss)
stylesheets[all][] = css/main.css

; Some legacy
stylesheets[all][] = css/shame.css

; Enabled drupal support
plugins[panels][layouts] = layouts

; Unset these stylesheets
settings[unset_styles][] = modules/system/system.menus.css
