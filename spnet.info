name = SPnet
description = Stand-alone SPnet theme
package = Other
version = 1.1
core = 7.x

; Available regions
regions[highlight] = Highlighted content
regions[content]   = Primary content
regions[secondary] = Secondary content
regions[footer]    = Footer content
regions[search]    = Search

scripts[] = js/vendor/modernizr-2.8.3.min.js
scripts[] = js/vendor/noframework.waypoints.min.js
scripts[] = js/main.js

; Fonts
stylesheets[all][] = fonts/SPnet-icons.css
